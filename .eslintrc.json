{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
       "linebreak-style": 0,
       "consistent-return": 0,
       "no-unused-vars": 1,
       "radix": 0,
       "no-undef": 0,
       "indent": ["error", 2],
       "no-console":0
    }
}